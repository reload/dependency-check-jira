{
    "name": "reload/dependency-check-jira",
    "description": "Create Jira tickets for CVEs",
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "symfony/process": "^5.0",
        "symfony/console": "^5",
        "reload/jira-security-issue": "dev-master",
        "phpstan/phpstan": "^0.12.9"
    },
    "require-dev": {
        "behat/behat": "^3.5",
        "phpunit/phpunit": "^9.0",
        "appocular/coding-standard": "^1.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/appocular/coding-standard"
        },
        {
            "type": "vcs",
            "url": "https://github.com/reload/jira-security-issue"
        }
    ],
    "autoload": {
        "psr-4": {
            "DependencyCheckJira\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DependencyCheckJira\\": "tests/",
            "": "features/bootstrap/"
        }
    }
}
