{
    "name": "reload/dependency-check-jira",
    "description": "Create Jira tickets for CVEs",
    "license": "MIT",
    "require": {
        "php": "^7.3 || ^8.0",
        "symfony/process": "^5.0",
        "symfony/console": "^5",
        "phpstan/phpstan": "^1.0.0",
        "reload/jira-security-issue": "^1.0.0"
    },
    "require-dev": {
        "behat/behat": "^3.5",
        "phpunit/phpunit": "^9.0",
        "appocular/coding-standard": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "DependencyCheckJira\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DependencyCheckJira\\": "tests/",
            "": "features/bootstrap/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
