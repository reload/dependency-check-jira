{
    "name": "reload/dependency-check-jira",
    "description": "Create Jira tickets for CVEs",
    "license": "MIT",
    "require": {
        "php": "^7.3 || ^8.0",
        "symfony/process": "^5.0",
        "symfony/console": "^5",
        "phpstan/phpstan": "^0.12.9",
        "reload/jira-security-issue": "^1.0.0"
    },
    "require-dev": {
        "behat/behat": "^3.5",
        "phpunit/phpunit": "^9.0",
        "appocular/coding-standard": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "DependencyCheckJira\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DependencyCheckJira\\": "tests/",
            "": "features/bootstrap/"
        }
    }
}
